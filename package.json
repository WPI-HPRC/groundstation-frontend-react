{
  "name": "hprc-groundstation",
  "version": "0.2.0",
  "private": true,
  "author": "WPI HPRC",
  "description": "WPI HPRC Telemetry Groundstation",
  "dependencies": {
    "@nivo/core": "^0.80.0",
    "@nivo/line": "^0.80.0",
    "@nivo/scatterplot": "^0.80.0",
    "@progress/kendo-drawing": "^1.16.2",
    "@progress/kendo-licensing": "^1.2.2",
    "@progress/kendo-react-gauges": "^5.0.1",
    "@progress/kendo-react-intl": "^5.0.1",
    "@progress/kendo-theme-default": "^5.1.1",
    "@ramonak/react-progress-bar": "^4.2.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "apexcharts": "^3.33.0",
    "axios": "^0.25.0",
    "bootstrap": "^5.1.3",
    "concurrently": "^7.0.0",
    "electron-is-dev": "^2.0.0",
    "esri-leaflet": "^3.0.4",
    "idb": "^7.0.0",
    "leaflet": "^1.7.1",
    "leaflet.offline": "^2.0.0-beta.3",
    "localforage": "^1.10.0",
    "nivo": "^0.31.0",
    "react": "^17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-bootstrap": "^2.1.1",
    "react-dom": "^17.0.2",
    "react-flexbox-grid": "^2.1.2",
    "react-leaflet": "^3.2.5",
    "react-rt-chart": "^2.0.0",
    "react-scripts": "5.0.0",
    "wait-on": "^6.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "electron": "^16.0.7"
  },
  "main": "./public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "set BROWSER=none&& concurrently -k \"npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "electron:package:mac": "npm run build && npx electron-builder -m -c.extraMetadata.main=public/electron.js",
    "electron:package:win": "npm run build && npx electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "npm run build && npx electron-builder -l -c.extraMetadata.main=public/electron.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.electron.groundstation",
    "productName": "HPRC Ground Station",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.png"
    },
    "linux": {
      "target": "deb"
    }
  }
}
